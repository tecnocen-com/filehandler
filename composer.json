{
    "name": "tecnocen/yii2-filehandler",
    "description": "Yii2 File handler validator",
    "keywords": [
        "yii",
        "yii2",
        "file",
        "filehandler",
        "file validator"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/tecnocen-com/filehandler",
    "authors": [
        {
            "name": "Angel (Faryshta) Guevara",
            "email": "aguevara@tecnocen.com",
            "homepage": "https://github.com/Faryshta",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "tecnocen\\filehandler\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
